    "chai-spies": "^1.0.0",
    "jsdoc-babel": "^0.5.0",
    "jsdoc-to-markdown": "^5.0.0",
    "mocha": "^7.1.2",
    "sinon": "^9.0.2",
    "sinon-chai": "^3.3.0",
    "terser-webpack-plugin": "^3.0.1",
    "ts-loader": "^7.0.4",
    "ts-node": "^8.10.1",
